<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lingzhong.video.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.lingzhong.video.bean.po.User">
            <id property="userId" column="user_id" jdbcType="INTEGER"/>
            <result property="userName" column="user_name" jdbcType="VARCHAR"/>
            <result property="userMail" column="user_mail" jdbcType="VARCHAR"/>
            <result property="userAccount" column="user_account" jdbcType="VARCHAR"/>
            <result property="userPassword" column="user_password" jdbcType="VARCHAR"/>
            <result property="userPhoto" column="user_photo" jdbcType="VARCHAR"/>
            <result property="userSex" column="user_sex" jdbcType="TINYINT"/>
            <result property="userDescribe" column="user_describe" jdbcType="VARCHAR"/>
            <result property="userDate" column="user_date" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        user_id,user_name,user_mail,user_account,
        user_password,user_photo,user_sex,
        user_describe,user_date
    </sql>
</mapper>
